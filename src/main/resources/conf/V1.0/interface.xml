<?xml version="1.0" encoding="UTF-8" ?>
<protocols>
	<!-- 充电站信息查询接口校验 -->
	<node id="StationInfos" type="object">
		<item name="ItemSize" type="int" value="总记录条数" isNull="no"/>
		<item name="PageCount" type="int" value="页码总数" isNull="no"/>
		<item name="PageNo" type="int" value="当前页数" isNull="no"/>
		<array name="StationInfos" isNull="no" value="充电站信息列表">
			<item name="StationID" type="String" value="充电站ID" isNull="no" maxlength="20"/>
			<item name="OperatorID" type="String" value="运营商ID" isNull="no" length="9"/>
			<item name="EquipmentOwnerID" type="String" value="设备所属方ID" isNull="no" length="9"/>
			<item name="StationName" type="String" value="充电站名称" isNull="no" maxlength="50"/>
			<item name="CountryCode" type="String" value="充电站国家代码" isNull="no" length="2"/>
			<item name="AreaCode" type="String" value="充电站省市辖区编码" isNull="no" maxlength="20"/>
			<item name="Address" type="String" value="详细地址" isNull="no" maxlength="50"/>
			<item name="StationTel" type="String" value="站点电话" isNull="no" maxlength="32"/>
			<item name="ServiceTel" type="String" value="服务电话" isNull="no" maxlength="30"/>
			<item name="StationType" type="enum" value="站点类型" isNull="no" format="int">
					<item name="1"  value="公共"/>
					<item name="50"  value="个人"/>
					<item name="100"  value="公交（专用）"/>
					<item name="101"  value="环卫（专用）"/>
					<item name="102"  value="物流（专用）"/>
					<item name="103"  value="出租车（专用）"/>
					<item name="255"  value="其他"/>
			</item>
			<item name="StationStatus" type="enum" value="站点状态" isNull="no" format="int">
					<item name="0"  value="未知"/>
					<item name="1"  value="建设中"/>
					<item name="5"  value="关闭下线"/>
					<item name="6"  value="维护中"/>
					<item name="50"  value="正常使用"/>
			</item>
			<item name="ParkNums" type="int" value="车位数量" isNull="no"/>
			<item name="StationLng" type="Double" value="经度" isNull="no"/>
			<item name="StationLat" type="Double" value="纬度" isNull="no"/>
			<item name="SiteGuide" type="String" value="站点引导" maxlength="100"/>
			<item name="Construction" type="enum" value="建设场所" isNull="no" format="int">
					<item name="1"  value="居民区"/>
					<item name="2"  value="公共机构"/>
					<item name="3"  value="企事业单位"/>
					<item name="4"  value="写字楼"/>
					<item name="5"  value="工业园区"/>
					<item name="6"  value="交通枢纽"/>
					<item name="7"  value="大型文体设施"/>
					<item name="8"  value="城市绿地"/>
					<item name="9"  value="大型建筑配建停车场"/>
					<item name="10"  value="路边停车位"/>
					<item name="11"  value="城际高速服务区"/>
					<item name="255"  value="其他"/>
			</item>
			<item name="Pictures" type="StringArray" isNull="no" value="站点照片"/>
			<item name="MatchCars" type="String" value="使用车型描述" maxlength="100"/>
			<item name="ParkInfo" type="String" value="车位楼层及数量描述" maxlength="100"/>
			<item name="BusineHours" type="String" value="营业时间" maxlength="100"/>
			
			
			<item name="ElectricityFee" type="String" value="充电电费率" maxlength="256"/>
			<item name="ServiceFee" type="String" value="服务费率" maxlength="256"/>
			<item name="ParkFee" type="String" value="停车费" maxlength="100"/>
			<item name="Payment" type="String" value="支付方式" maxlength="20"/>
			<item name="SupportOrder" type="enum" value="是否支持预约" format="int">
					<item name="0"  value="不支持预约"/>
					<item name="1"  value="支持预约"/>
			</item>
			<item name="Remark" type="String" value="备注" maxlength="100"/>
			<item name="ParkOwner" type="String" value="停车场产权方" />
			<item name="ParkManager" type="String" value="停车场管理方" />
			<item name="OpenAllDay" type="enum" value="全天开放" format="int">
					<item name="0"  value="否"/>
					<item name="1"  value="是"/>
			</item>
			<item name="ParkFree" type="enum" value="免费停车" format="int">
					<item name="0"  value="否"/>
					<item name="1"  value="是"/>
			</item>
			<item name="OpratorType" type="enum" value="运营属性" isNull="no" format="int">
					<item name="1"  value="由本运营商自营"/>
					<item name="2"  value="由本运营商代运营"/>
			</item>
			<array name="EquipmentInfos" value="充电设备信息列表">
				<item name="EquipmentID" type="String" value="设备编码" isNull="no" maxlength="23"/>
				<item name="EquipmentName" type="String" value="设备名称" isNull="no" maxlength="30"/>
				<item name="ManufacturerID" type="String" value="设备生产商组织机构代码" isNull="no" length="9"/>
				<item name="EquipmentModel" type="String" value="设备型号"  maxlength="20"/>
				<item name="ProductionDate" type="Date" value="设备生产日期" format="yyyy-MM-dd"/>
				<item name="EquipmentType" type="enum" value="设备类型" isNull="no" format="int">
					<item name="0"  value="直流设备"/>
					<item name="1"  value="交流设备"/>
					<item name="2"  value="交流设备"/>
					<item name="3"  value="交直流一体设备"/>
					<item name="4"  value="无线充电"/>
					<item name="5"  value="充放电设备"/>
					<item name="255"  value="其他"/>
				</item>
				<item name="EquipmentLng" type="Double" value="充电设备经度" />
				<item name="EquipmentLat" type="Double" value="充电设备纬度" />
				<item name="Power" type="Double" value="充电设备总功率" isNull="no"/> 
				<item name="ManufacturerName" type="String" value="设备生产商的名称" maxlength="30"/>
				<item name="EquipmentStatus" type="enum" value="设备状态" isNull="no" format="int">
					<item name="1"  value="建设中"/>
					<item name="5"  value="关闭下线：停用"/>
					<item name="6"  value="维护中：长时间维护"/>
					<item name="50"  value="正常使用，验收后"/>
				</item>
				<item name="EquipmentPower" type="Double" value="额定功率" isNull="no"/>
				<item name="NewNationalStandard" type="enum" value="支持标准版本"  format="int" isNull="no">
					<item name="1"  value="2011版"/>
					<item name="2"  value="2015版"/>
					<item name="3"  value="大功率交流"/>
					<item name="255"  value="其他"/>
				</item>
				<array name="ConnectorInfos" value="充电设备接口列表" isNull="no">
					<item name="ConnectorID" type="String" value="充电设备接口编码" isNull="no" maxlength="30"/>
					<item name="ConnectorName" type="String" value="充电设备接口名称" maxlength="30" isNull="no"/>
					<item name="ConnectorType" type="enum" value="充电设备接口类型" isNull="no" format="int">
						<item name="1"  value="家用插座（模式2）"/>
						<item name="2"  value="交流接口插座（模式3，连接方式B ）"/>
						<item name="3"  value="交流接口插头（带枪线，模式3，连接方式C）"/>
						<item name="4"  value="直流接口插头（带枪线，模式4）"/>
						<item name="5"  value="大功率交流接口插头"/>
						<item name="255"  value="其他"/>
					</item>
					<item name="VoltageUpperLimits" type="int" value="最高输出电压" isNull="no"/>
					<item name="VoltageLowerLimits" type="int" value="最低输出电压" isNull="no"/>
					<item name="Current" type="int" value="额定电流" isNull="no"/>
					<item name="Power" type="Double" value="额定功率" isNull="no"/> 
					<item name="ParkNo" type="String" value="车位号" maxlength="10"/>
					<item name="Voltage" type="int" value="额定电压" isNull="no"/>
					<item name="BMSPowerType" type="enum" value="BMS辅助电源电压" format="int" isNull="no">
						<item name="1"  value="12V"/>
						<item name="2"  value="24V"/>
						<item name="3"  value="自适应"/>
						<item name="255"  value="其他"/>
					</item>
				</array>
			</array>
		</array>
	</node>
